	margin: 0px 2px 0px 2px;
	subcontrol-origin: margin;
	
	border-width: 6 1 6 1;
	border-image: url(:/MoodBox/Resources/scrollbar_vertical_slider.png) 6 1 6 1 stretch repeat;
}

