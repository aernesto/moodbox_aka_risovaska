	image: url(:/MoodBox/Resources/button_draw_hover.png);
}

QToolButton#drawButton:pressed, QToolButton#drawButton:checked
{
	image: url(:/MoodBox/Resources/button_draw_pressed.png);
}

#menuButton
{
	border: none;
	image: url(:/MoodBox/Resources/button_cl_menu.png);
}

#menuButton:hover
{
	image: url(:/MoodBox/Resources/button_cl_menu_hover.png);
}

#menuButton:pressed, #menuButton:checked
{
	image: url(:/MoodBox/Resources/button_cl_menu_pressed.png);
}

#menuButton::menu-indicator
{
	image: none;
}

#textStyleButton1
{
	font: normal normal 12px "Arial";
}

#textStyleButton2
{
	font: normal normal 12px "Verdana";
}

#textStyleButton3
{
	font: normal normal 12px "Comic Sans MS";
}

#styleButton
{
	font: normal normal 12px "Arial";
}


QToolButton[accessibleName="infoButton"]
{
	border: none;
	image: none;
}

QToolButton[accessibleName="infoButton"]:pressed
{
	padding: 0;
}

#StatusButtonClass
{	
	max-height: 34px;
	min-height: 34px;

	border: none;
	background: url(:/MoodBox/Resources/cl_menu_button.png);
}

#contactAvatarToolButton:pressed
{
	padding: 0px;
}

#StatusButtonClass #textLabel, #StatusButtonClass #arrowLabel
{
	padding-top: 6px;
}

QPushButton#addChannelButton
{
	image: url(:/MoodBox/Resources/button_add.png);
}
QPushButton#addChannelButton:hover
{
	image: url(:/MoodBox/Resources/button_add.png);
}
QPushButton#addChannelButton:pressed
{
	image: url(:/MoodBox/Resources/button_add_pressed.png);
}
QPushButton#addChannelButton:disabled
{
	image: url(:/MoodBox/Resources/button_add_pressed.png);
}

QPushButton#removeChannelButton
{
	image: url(:/MoodBox/Resources/button_remove.png);
}
QPushButton#removeChannelButton:hover
{
	image: url(:/MoodBox/Resources/button_remove.png);
}
QPushButton#removeChannelButton:pressed
{
	image: url(:/MoodBox/Resources/button_remove_pressed.png);
}
QPushButton#removeChannelButton:disabled
{
	image: url(:/MoodBox/Resources/button_remove_pressed.png);
}

/*ContactList Menu******************************/

#ContactListMenuClass #menuFrame
{
	border-width: 5;
	border-image: url(:/MoodBox/Resources/cl_menu_background.png) 5 5 5 5 stretch, stretch;
}

#ContactListMenuClass #separatorFrame
{
	height: 2px;
	border: none;
	background: url(:/MoodBox/Resources/cl_menu_separator.png) repeat-x;
}

#ContactListMenuClass QToolButton
{
	border: none;
	height: 32px;
	
	font: bold normal 13px "Verdana";
	
	image-position: left center;
	padding-left: 5px;
}

#ContactListMenuClass QToolButton:hover
{
	border: none;
	background: url(:/MoodBox/Resources/cl_menu_item_selected.png) repeat-x;
}

#ContactListMenuClass #onlineButton
{
	color: rgb(126, 208, 10);
	image: url(:/MoodBox/Resources/tv_online.png);
}
#ContactListMenuClass #onlineButton:disabled
{
	color: gray;
	image: url(:/MoodBox/Resources/tv_online_disabled.png);
}

#ContactListMenuClass #offlineButton
{
	color: rgb(255, 45, 104);
	image: url(:/MoodBox/Resources/tv_offline.png);
}
#ContactListMenuClass #offlineButton:disabled
{
	color: gray;
	image: url(:/MoodBox/Resources/tv_offline_disabled.png);
}

#ContactListMenuClass #logoutButton
{
	color: rgb(0, 177, 252);
	image: url(:/MoodBox/Resources/tv_logout.png);
}

#ContactListMenuClass #exitButton
{
	color: rgb(255, 203, 31);
	image: url(:/MoodBox/Resources/tv_exit.png);
}

#ContactListMenuClass #onlineButton:disabled, 
#ContactListMenuClass #offlineButton:disabled
{
	color: grey;
}



/* List views *******************************/

#ClipartWindowClass QListView, #HistoryWindowClass QListView
{
	background-color: transparent;
	border: none;
	margin: 0;
	padding: 0;
}

/*QSizeGrip***************************************************/

QSizeGrip
{
	image: url(:/MoodBox/Resources/size_grip.png);
	
	width: 19px;
	height: 19px;
}
