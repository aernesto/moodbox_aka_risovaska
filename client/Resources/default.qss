/*Check Box****************************************************/

QCheckBox 
{
     spacing: 5px;
}

QCheckBox::indicator 
{
	width: 13px;
	height: 13px;
}

QCheckBox::indicator:unchecked 
{
	image: url(:/MoodBox/Resources/checkbox_unchecked.png);
}

QCheckBox::indicator:unchecked:hover 
{
	image: url(:/MoodBox/Resources/checkbox_unchecked_hover.png);
}

QCheckBox::indicator:unchecked:pressed 
{
	image: url(:/MoodBox/Resources/checkbox_unchecked_pressed.png);
}

QCheckBox::indicator:checked
{
	image: url(:/MoodBox/Resources/checkbox_checked.png);
}

QCheckBox::indicator:checked:hover 
{
	image: url(:/MoodBox/Resources/checkbox_checked_hover.png);
}

QCheckBox::indicator:checked:pressed 
{
	image: url(:/MoodBox/Resources/checkbox_checked_pressed.png);
}

QCheckBox::indicator:unchecked:disabled, QCheckBox::indicator:checked:disabled
{
	image: url(:/MoodBox/Resources/checkbox_disabled.png);
}

