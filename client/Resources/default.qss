/*Scroll bars*****************************************/

QScrollBar:vertical
{
	border-width: 5 1 4 1;
	border-image: url(:/MoodBox/Resources/scrollbar_vertical.png) 5 1 4 1 stretch repeat;

	width: 11px;
}

QScrollBar::handle:vertical
{
	min-height: 50px;
	
	margin: 0px 1px 0px 1px;
	subcontrol-origin: margin;
	
	border-width: 6 1 6 1;
	border-image: url(:/MoodBox/Resources/scrollbar_vertical_slider.png) 6 1 6 1 stretch repeat;
}

QScrollBar::handle:vertical:disabled
{	
	border-image: none;
}

QScrollBar::add-page:vertical, QScrollBar::sub-page:vertical 
{
	background: none;
}

QScrollBar::sub-line:vertical
{
	background: none;
	height: 0px;
	subcontrol-position: top;
	subcontrol-origin: margin;
}

QScrollBar::add-line:vertical
{
	background: none;
	height: 0px;
	subcontrol-position: bottom;
	subcontrol-origin: margin;
}

#NewContactListWindowClass QScrollBar:vertical
{
	border-width: 0 2 0 2;
	border-image: url(:/MoodBox/Resources/cl_scrollbar_vertical_page.png) 0 2 0 2 stretch repeat;

	width: 13px;
}

#FindPeopleFrameClass QScrollBar:vertical
{
	border-width: 5 1 4 1;
	border-image: url(:/MoodBox/Resources/scrollbar_vertical.png) 5 1 4 1 stretch repeat;

	width: 11px;
}

#FindPeopleFrameClass QScrollBar::handle:vertical
{
	min-height: 50px;
	
	margin: 0px 1px 0px 1px;
	subcontrol-origin: margin;
	
	border-width: 6 1 6 1;
	border-image: url(:/MoodBox/Resources/scrollbar_vertical_slider.png) 6 1 6 1 stretch repeat;
}

#NewContactListWindowClass QScrollBar::handle:vertical
{
